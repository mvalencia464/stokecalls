# HighLevel API Configuration
# Get your Location ID from: https://app.gohighlevel.com/location/settings
NEXT_PUBLIC_GHL_LOCATION_ID=your_location_id_here

# Private Registration Access Token
# Generate from: https://marketplace.gohighlevel.com/oauth/chooselocation
GHL_ACCESS_TOKEN=your_ghl_access_token_here

# AssemblyAI for transcription (speaker labels only)
ASSEMBLYAI_API_KEY=your_assemblyai_api_key_here

# Google Gemini API for AI analysis (summary, sentiment, Q&A)
# Get your key at: https://aistudio.google.com/app/apikey
GEMINI_API_KEY=your_gemini_api_key_here

# Optional: OpenAI for AI features (currently using Gemini instead)
OPENAI_API_KEY=your_openai_api_key_here

# Supabase Configuration
# Get these from: https://app.supabase.com/project/_/settings/api
NEXT_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key_here
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key_here

# App URL (for webhooks)
NEXT_PUBLIC_APP_URL=https://stokecalls.netlify.app

